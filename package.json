{
  "name": "ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "preinstall": "node ./preinstall.js",
    "dev": "parcel src/index.html",
    "storybook": "start-storybook --loglevel verbose -p 6006",
    "start": "npm run dev & npm run storybook ",
    "build": "npm run build-storybook && npm run build-parcel",
    "build-storybook": "build-storybook --output-dir dist",
    "build-storybook-serve": "build-storybook --output-dir dist && serve ./dist",
    "build-parcel": "parcel build src/index.html",
    "build-parcel-serve": "parcel build src/index.html && serve ./dist",
    "serve": "serve ./dist"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "stories": "./src/stories",
    "default.settings": "./src/default.settings.js",
    "app.settings": "./src/app.settings.js"
  },
  "dependencies": {
    "@storybook/cli": "^6.2.9",
    "@types/styled-components": "^5.1.3",
    "babel-plugin-styled-components": "^1.11.1",
    "htm": "^3.0.4",
    "parcel-bundler": "^1.12.4",
    "preact": "^10.5.13",
    "preact-context": ">=1.1.0",
    "preact-router": "^3.2.1",
    "react-redux": "^7.2.1",
    "redux": ">=2",
    "serve": "^11.3.2",
    "src": "^1.1.2",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "styled": "^1.0.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preact": "^6.2.9",
    "babel-loader": "^8.2.2",
    "core-js": "^3.9.1",
    "typescript": "^4.0.3"
  }
}
